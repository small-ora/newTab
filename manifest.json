{
  "manifest_version": 3,
  "name": "ChengStartPage",
  "version": "1.0",
  "description": "一个极简、支持自定义图标和壁纸切换的起始页。",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "128": "icon.png" 
  },
  "action": {
    "default_icon": "icon.png"
  }
}